{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "employees": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "first_name": {
                        "type": "string",
                        "maxLength": 100
                    },
                    "last_name": {
                        "type": "string",
                        "maxLength": 100
                    },
                    "certification_number": {
                        "type": "string",
                        "maxLength": 50
                    },
                    "certification_state": {
                        "enum": [
                            "AL", "AK", "AS", "AZ", "AR", "CA", "CO", "CT", "DE", "DC", "FL", "GA", "GU", "HI", "ID", "IL", "IN", "IA", 
                            "KS", "KY", "LA", "ME", "MD", "MA", "MI", "MN", "MS", "MO", "MT", "NE", "NV", "NH", "NJ", "NM", "NY", "NC", 
                            "ND", "OH", "OK", "OR", "PA", "PR", "RI", "SC", "SD", "TN", "TX", "UT", "VT", "VA", "VI", "WA", "WV", "WI", "WY"
                          ]
                    },
                    "alt_certification_state": {
                        "enum": [
                            "AL", "AK", "AS", "AZ", "AR", "CA", "CO", "CT", "DE", "DC", "FL", "GA", "GU", "HI", "ID", "IL", "IN", "IA", 
                            "KS", "KY", "LA", "ME", "MD", "MA", "MI", "MN", "MS", "MO", "MT", "NE", "NV", "NH", "NJ", "NM", "NY", "NC", 
                            "ND", "OH", "OK", "OR", "PA", "PR", "RI", "SC", "SD", "TN", "TX", "UT", "VT", "VA", "VI", "WA", "WV", "WI", "WY"
                          ]
                    },
                    "middle_name": {
                        "type": "string",
                        "maxLength": 100
                    },
                    "maiden_name": {
                        "type": "string",
                        "maxLength": 100
                    },
                    "alt_last_name": {
                        "type": "string",
                        "maxLength": 100
                    },
                    "custom_id": {
                        "type": "string",
                        "maxLength": 50
                    },
                    "facility_id": {
                        "type": "string",
                        "maxLength": 65
                    },
                    "date_of_birth": {
                        "type": "string",
                        "format": "date",
                        "maxLength": 10,
                        "description": "Date of birth in YYYY-MM-DD format"
                    },
                    "upin": {
                        "type": "string",
                        "maxLength": 50
                    },
                    "npi": {
                        "type": "string",
                        "minLength": 10,
                        "maxLength": 10
                    },
                    "social_security_num": {
                        "type": "string",
                        "minLength": 9,
                        "maxLength": 9
                    },
                    "certification_board": {
                        "enum": [
                            "aacn", "aaet", "aahncr", "aama", "aapc", "aba", "abai", "abcop", "abcrs", "abfas", "abfm", "abha", "abim", 
                            "abms", "abns", "abo", "abog", "abop", "abos", "aboto", "abp", "abpm", "abpmed", "abplm", "abpmr", "abpn", 
                            "abps", "abr", "abret", "absm", "absurgery", "abts", "abu", "abvm", "action", "ahima", "amalpp", "ambd", 
                            "ambem", "ambo", "ambom", "ambp", "ambpd", "ambperio", "ambpm", "amcb", "amcci", "amend", "amha", "ammdcb", 
                            "amnasm", "amncctinc", "amnmtcb", "amnncc", "amrdms", "amredc", "amrrt", "amtech", "asha", "atcb", "bacb", 
                            "bocat", "bocusa", "bps", "brpt", "cbdce", "cbmt", "ccmc", "cdr", "hpcc", "hspa", "iblce", "naham", "nahwa", 
                            "nbcc", "nbcot", "nbcrna", "nbrc", "nbstsa", "nccaom", "nccpa", "nremt", "oncc", "ptcb", "ptcp", "resna", 
                            "usreps", "vacb"
                        ]
                    },
                    "license_type": {
                        "type": "string",
                        "maxLength": 200
                    },
                    "alt_certification_board": {
                        "type": "string",
                        "maxLength": 10
                    },
                    "address1": {
                        "type": "string",
                        "maxLength": 128
                    },
                    "address2": {
                        "type": "string",
                        "maxLength": 128
                    },
                    "city": {
                        "type": "string",
                        "maxLength": 65
                    },
                    "state": {
                        "type": "string",
                        "maxLength": 65
                    },
                    "zip": {
                        "type": "string",
                        "maxLength": 10
                    },
                    "job_title": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "record_status": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "date_hire": {
                        "type": "string",
                        "maxLength": 10,
                        "description": "Date of birth in YYYY-MM-DD format"
                    },
                    "notes": {
                        "type": "string",
                        "maxLength": 65535
                    },
                    "dea_number": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "csl_number": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "mmis_number": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "alt_csl_number": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "alt_csl_state": {
                        "enum": [
                            "AL", "AK", "AS", "AZ", "AR", "CA", "CO", "CT", "DE", "DC", "FL", "GA", "GU", "HI", "ID", "IL", "IN", "IA", 
                            "KS", "KY", "LA", "ME", "MD", "MA", "MI", "MN", "MS", "MO", "MT", "NE", "NV", "NH", "NJ", "NM", "NY", "NC", 
                            "ND", "OH", "OK", "OR", "PA", "PR", "RI", "SC", "SD", "TN", "TX", "UT", "VT", "VA", "VI", "WA", "WV", "WI", "WY"
                        ]
                    }
                },
                "required": [
                    "certification_number",
                    "certification_state",
                    "first_name",
                    "last_name"
                ]
            },
            "maxContains": 1000
        },
        "entities": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "business": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "custom_id": {
                        "type": "string",
                        "maxLength": 50
                    },
                    "alt_business1": {
                        "type": "string",
                        "maxLength": 100
                    },
                    "alt_business2": {
                        "type": "string",
                        "maxLength": 100
                    },
                    "alt_last_name": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "upin": {
                        "type": "string",
                        "maxLength": 50
                    },
                    "npi": {
                        "type": "string",
                        "minLength": 10,
                        "maxLength": 10
                    },
                    "social_security_num": {
                        "type": "string",
                        "minLength": 9,
                        "maxLength": 9
                    },
                    "facility_id": {
                        "type": "string",
                        "maxLength": 65
                    },
                    "record_status": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "mmis_number": {
                        "type": "string",
                        "maxLength": 255
                    }
                },
                "required": [
                    "business"
                ]
            },
            "maxContains": 1000
        },
        "edit_details": {
            "oneOf": [
                { "type": "boolean", "default": false },
                { "type": "integer", "enum": [0, 1], "default": 0 }
            ]
        },
        "employeelist_id": {
            "type": "integer",
            "maxLength": 10,
            "minimum": 1,
            "description": "Optional. For advanced usage, employeelist_id can be added but is not required."
        }
    },
    "oneOf": [
        { "required": ["employees"] },
        { "required": ["entities"] }
    ]
}
